Comment: 0,0:21:45.44,0:21:50.03,Shiritori,,0,0,0,code once,function findtag(text, tagtype, startindex, endindex)  local valuestarts = "[0123456789%&%-%(]"  if tagtype == nil then tagtype = "[^\\%}]"; valuestarts = "[^\\%}]" end  if tagtype == "fn" then valuestarts = "[^\\%}]" end  if tagtype == "t" then valuestarts = "[(]" end  if tagtype == "inline_fx" then tagtype = "%-"; valuestarts = "[^\\%}]"; end  if tagtype == "c" or tagtype == "1c" then tagtype = "1?c" end  if startindex == nil then startindex = 1 end  if endindex == nil then endindex = -1 end  if text == nil then return 0,0 end  tagstart = text:find(("\\%s%s"):format(tagtype,valuestarts), startindex)  if tagstart == nil then return 0,0 end  local depth = 0  for i=startindex,tagstart do    local c = text:sub(i,i)    if c == "(" then depth = depth + 1 end    if c == ")" then depth = depth - 1 end  end  if depth ~= 0 then    return 0,0  end  depth = 0  local max_i = endindex  if max_i < 0 then max_i = text:len() + 1 + max_i end  for i = tagstart + 1, math.min(text:len(), max_i) do    local c = text:sub(i, i)    if c == "(" then depth = depth + 1 end    if c == ")" then depth = depth - 1 end       if c == "}" or (c == "\\" and depth == 0) then      return tagstart, i - 1    end  end  return 0,0 end
Comment: 0,0:21:45.44,0:21:50.03,Shiritori,,0,0,0,code once,local pairs, ipairs, table = _G.pairs, _G.ipairs, _G.table;function findtags(text, tags);	local ret = {};	for i, tag in ipairs(tags) do;		for ci = 1, #text do;			local si, ei = findtag(text, tag, ci);			if ei and ei ~= 0 then;				ci = ei+1;				ret[tag] = text:sub(si, ei);			else;				break;			end;		end;	end;	return ret;end;function footags(tags);	local foo = findtags(tenv.line.text, tags);	for k, v in pairs(findtags(tenv.syll.text, tags)) do;		foo[k] = v;	end;	local ret = {};	for k, v in pairs(foo) do;		table.insert(ret, v);	end;	return table.concat(ret);end;
